qmake: 
	@$(QMAKE) -o Makefile decmpa.pro

dist: 
	@mkdir -p .obj/decmpa && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/decmpa/ && ( cd `dirname .obj/decmpa` && $(TAR) decmpa.tar decmpa && $(GZIP) decmpa.tar ) && $(MOVE) `dirname .obj/decmpa`/decmpa.tar.gz . && $(DEL_FILE) -r .obj/decmpa

